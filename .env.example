# Database Configuration
DATABASE_URL="postgresql://user:password@host:port/database"
RAILWAY_DATABASE_URL="postgresql://user:password@host:port/database"

# Admin Panel Authentication (DO NOT COMMIT REAL VALUES)
ADMIN_PASSWORD=your_secure_password_here
ADMIN_SESSION_SECRET=your_64_character_random_secret_here

# Admin Panel Control (set to 'true' only for development/staging)
# Leave unset or set to 'false' for production
ENABLE_ADMIN_PANEL=false

# Google Gemini AI (optional - for AI features)
GOOGLE_AI_API_KEY=your_api_key_here

# Environment
NODE_ENV=production
